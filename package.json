{
  "name": "tabstats",
  "version": "1.2.5",
  "description": "An extension that track amount of time you spent on a website each day.",
  "extensionMain": "./src/extension/index.js",
  "clientMain": "./src/client/index.js",
  "scripts": {
    "build-extension": "webpack --mode production --config webpack.extension/webpack.prod.config.js",
    "build-client-prod": "webpack --mode production --config webpack.client/webpack.prod.config.js",
    "run-client-dev": "npm run sym-copy-essentials && webpack-dev-server --mode development --config webpack.client/webpack.dev.config.js --devtool inline-source-map --hot",
    "copy-essentials": "cp -rip node_modules/jquery extension/cdn/jquery && cp -rip node_modules/bootstrap extension/cdn/bootstrap && cp webpack.client/dist/bundle.js extension/cdn/bundle.js && cp webpack.extension/dist/background.js extension/background.js",
    "sym-copy-essentials": "ln -s $(pwd)/node_modules/jquery $(pwd)/extension/cdn/jquery && ln -s $(pwd)/node_modules/bootstrap $(pwd)/extension/cdn/bootstrap && ln -s $(pwd)/webpack.client/dist/bundle.js $(pwd)/extension/cdn/bundle.js && ln -s $(pwd)/webpack.extension/dist/background.js $(pwd)/extension/background.js",
    "build-and-compress-extension": "npm run clean && npm run build-client-prod && npm run build-extension && npm run copy-essentials && zip -r extension.zip extension",
    "clean": "rm -r webpack.client/dist/bundle.js || true ; rm -r webpack.extension/dist/background.js || true ; rm -r extension/cdn/jquery || true ; rm -r extension/cdn/bootstrap || true ; rm extension/cdn/bundle.js || true ; rm extension/background.js || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pallab-gain/TabStats.git"
  },
  "keywords": [
    "tab",
    "chrome",
    "extension",
    "time",
    "management",
    "track"
  ],
  "author": "pallab.gain@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pallab-gain/TabStats/issues"
  },
  "homepage": "https://github.com/pallab-gain/TabStats#README.md",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.0.9",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.3",
    "mocha": "^7.0.1",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "highcharts": "^8.0.0",
    "highcharts-react-official": "^3.0.0",
    "intl": "^1.2.5",
    "jquery": "^3.4.1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-login": "^5.1.1",
    "react-intl": "^3.12.0",
    "react-load-script": "0.0.6",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  }
}
