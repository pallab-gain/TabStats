{
  "name": "tabstats",
  "version": "1.0.2",
  "description": "An extension that track amount of time you spent on a website each day.",
  "extensionMain": "./src/extension/index.js",
  "clientMain": "./src/client/index.js",
  "scripts": {
    "build-extension": "webpack --mode production --config webpack.extension/webpack.prod.config.js",
    "build-client-prod": "webpack --mode production --config webpack.client/webpack.prod.config.js",
    "build-client-dev": "npm run clean || npm run sym-copy-essentials && webpack-dev-server --mode development --config webpack.client/webpack.dev.config.js --devtool inline-source-map --hot",
    "copy-essentials": "cp -r node_modules/jquery extension/cdn/jquery && cp -r node_modules/bootstrap extension/cdn/bootstrap && cp webpack.client/dist/bundle.js extension/cdn/bundle.js && cp webpack.extension/dist/background.js extension/background.js",
    "sym-copy-essentials": "ln -s $(pwd)/node_modules/jquery $(pwd)/extension/cdn/jquery && ln -s $(pwd)/node_modules/bootstrap $(pwd)/extension/cdn/bootstrap && ln -s $(pwd)/webpack.client/dist/bundle.js $(pwd)/extension/cdn/bundle.js && ln -s $(pwd)/webpack.extension/dist/background.js $(pwd)/extension/background.js",
    "build-and-compress-extension": "npm run clean || npm run build-client-prod && npm run build-extension && npm run copy-essentials && zip -r extension.zip extension",
    "clean": "rm -r extension/cdn/jquery extension/cdn/bootstrap extension/cdn/bundle.js extension/background.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pallab-gain/TabStats.git"
  },
  "keywords": [
    "tab",
    "chrome",
    "extension",
    "time",
    "management",
    "track"
  ],
  "author": "pallab.gain@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pallab-gain/TabStats/issues"
  },
  "homepage": "https://github.com/pallab-gain/TabStats#README.md",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "style-loader": "^1.0.0",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "highcharts": "^7.1.3",
    "highcharts-react-official": "^2.2.2",
    "intl": "^1.2.5",
    "jquery": "^3.4.1",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-google-login": "^5.0.4",
    "react-intl": "^2.9.0",
    "react-load-script": "0.0.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  }
}
